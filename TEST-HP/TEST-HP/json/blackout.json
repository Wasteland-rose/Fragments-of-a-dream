{
  "loginScene": {
    "onEnter": {
      "effect": "blackout",
      "duration": 1000,
      "easing": "ease-in-out"
    },
    "transition": {
      "to": "brightScene",
      "delay": 500
    },
    "brightScene": {
      "background": "clearSky",
      "brightness": 1.0,
      "fadeInDuration": 1500,
      "easing": "ease-in"
    }
  }
}
